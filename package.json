{
    "name": "rest_ts",
    "version": "1.0.0",
    "description": "Base Rest API with TypeScript + NodeJs",
    "main": "app.ts",
    "scripts": {
        "tsc": "tsc",
        "serve": "node --inspect dist/app/app.js",
        "local": "npm run lint && tsc && concurrently \"tsc -w\" \"nodemon dist/app/app.js\"",
        "queue:work": "node --inspect dist/app/worker.js",
        "queue:listen": "npm run lint && concurrently \"tsc -w\" \"nodemon dist/app/worker.js\"",
        "build": "npm run lint && tsc",
        "lint": "eslint \"**/*.{js,ts}\" --quiet --fix",
        "sequelize": "ts-node ./node_modules/.bin/sequelize",
        "tinker": "ts-node -r module-alias/register -r reflect-metadata -r ./src/bootstrap/boot --project tsconfig.json --interactive",
        "cmd": "node dist/app/Console/Kernel.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tqvthu-works/shopify_restful.git"
    },
    "keywords": [
        "restful",
        "shopify_api"
    ],
    "author": "tqvthu.works",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/tqvthu-works/shopify_restful/issues"
    },
    "_moduleAliases": {
        "@config": "./dist/config",
        "@app": "./dist/app",
        "@database": "./dist/database",
        "@routes": "./dist/routes",
        "@contract": "./dist/contract",
        "@constant": "./dist/constant",
        "@bootstrap": "./dist/bootstrap",
        "@core": "./dist/core"
    },
    "homepage": "https://github.com/tqvthu/rest_ts#readme",
    "dependencies": {
        "@sentry/node": "^7.38.0",
        "@sentry/tracing": "^7.38.0",
        "axios": "^1.3.6",
        "bull": "^4.10.4",
        "commander": "^10.0.1",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-validator": "^6.15.0",
        "http-status": "^1.6.2",
        "i18next": "^22.4.15",
        "inversify": "^6.0.1",
        "ioredis": "^5.3.2",
        "jsonwebtoken": "^9.0.0",
        "lodash": "^4.17.21",
        "module-alias": "^2.2.2",
        "moment": "^2.29.4",
        "mysql2": "^3.1.2",
        "nodemon": "^2.0.20",
        "reflect-metadata": "^0.1.13",
        "sequelize": "^6.28.0",
        "typescript": "^4.9.5",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/eslint": "^8.21.1",
        "@types/express": "^4.17.17",
        "@types/ioredis": "^5.0.0",
        "@types/node": "^18.14.0",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "concurrently": "^7.6.0",
        "console-log-colors": "^0.3.5",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "install": "^0.13.0",
        "npm": "^9.5.1",
        "prettier": "^2.8.8",
        "sequelize-cli": "6.3.0",
        "ts-node": "^10.9.1"
    }
}
